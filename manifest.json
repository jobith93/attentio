{
	"name" : "Attentio - Grab your team-mate's attention!",
	"version" : "1.2.1",
	"description" : "Grab your team-mate's attention!",
	"permissions": ["tabs", "activeTab", "declarativeContent", "storage", "notifications"],
	"background": {
		"scripts": ["socket.io.js", "background.js"],
		"persistent": true
	},
	"page_action": {
		"default_popup": "popup/index.html",
		"default_icon": {
			"16": "images/icons/attentio16.png",
			"32": "images/icons/attentio32.png",
			"48": "images/icons/attentio48.png",
			"128": "images/icons/attentio128.png"
		}
	},
	"icons" : {
		"16" : "images/icons/attentio16.png",
		"32" : "images/icons/attentio32.png",
		"48" : "images/icons/attentio48.png",
		"128": "images/icons/attentio128.png"
	},
	"web_accessible_resources": [
		"audio/notify.mp3"
	],
	"content_security_policy": "img-src 'self' http://206.189.136.1:3002/static/images/; object-src 'self'; script-src 'self'",
	"incognito" : "split",
	"manifest_version" : 2
}
